	<queryString>
		<![CDATA[SELECT
     tesis.`titulo` AS tesis_titulo,
     tesis.`status` AS tesis_status,
     tesis.`fecha_inicio` AS tesis_fecha_inicio,
     tesis.`fecha_fin` AS tesis_fecha_fin,
     tesis.`observaciones` AS tesis_observaciones,
     tesis.`departamento` AS tesis_departamento,
     estudiante.`nombre` AS estudiante_nombre,
     estudiante.`apellido` AS estudiante_apellido,
     profesor.`nombre` AS profesor_nombre,
     profesor.`apellido` AS profesor_apellido,
     tutor_industrial.`nombre` AS tutor_industrial_nombre,
     tutor_industrial.`apellido` AS tutor_industrial_apellido,
     empresa.`nombre` AS empresa_nombre,
     notas.`tutor_academico` AS notas_tutor_academico,
     notas.`tutor_industrial` AS notas_tutor_industrial,
     notas.`promedio_defensa` AS notas_promedio_defensa,
     notas.`final` AS notas_final,
     estudiante.`idestudiante` AS estudiante_idestudiante,
     tesis.`idtesis` AS tesis_idtesis,
     profesor.`idprofesor` AS profesor_idprofesor,
     tutor_industrial.`idtindustrial` AS tutor_industrial_idtindustrial
FROM
     `estudiante` estudiante INNER JOIN `tesis` tesis ON estudiante.`idestudiante` = tesis.`estudiante_tesis`
     INNER JOIN `profesor` profesor ON tesis.`id_tutorAcademico` = profesor.`idprofesor`
     INNER JOIN `tutor_industrial` tutor_industrial ON tesis.`id_tutorIndustrial` = tutor_industrial.`idtindustrial`
     INNER JOIN `notas` notas ON tesis.`idtesis` = notas.`id_tesis`
     INNER JOIN `empresa` empresa ON tutor_industrial.`id_empresa` = empresa.`idempresa`
     AND empresa.`idempresa` = estudiante.`id_empresa`]]>
	</queryString>
